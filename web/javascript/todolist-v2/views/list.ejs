<%- include("header"); -%>
        <div class="box" id="heading">
            <h1><%= listName %></h1>
        </div>
        <div class="box">
            <% for (var i = 0; i < items.length; i++) {%>
            <div class="item">
                <input type="checkbox">
                <p><%= items[i] %></p>   
            </div>
            <% } %>
            <form class="item" action="/" method="post">
                <input type="text" name="item" placeholder="New item" autocomplete="off">
                <button name="list" value=<%= listName %> type="submit">+</button>
            </form>
        </div>
<%- include("footer"); -%>
